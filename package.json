{
  "name": "quick-remix",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier \"**/*.+(js|jsx|css|ts|tsx|cjs|mjs)\"",
    "format": "pnpm prettier --list-different",
    "format:write": "pnpm prettier --write",
    "start": "remix-serve ./build/server/index.js",
    "preview": "pnpm build && pnpm start",
    "typecheck": "tsc",
    "prepare": "husky",
    "validate": "run-p typecheck lint format"
  },
  "dependencies": {
    "@conform-to/react": "^1.1.5",
    "@conform-to/zod": "^1.1.5",
    "@epic-web/client-hints": "^1.3.2",
    "@remix-run/node": "^2.10.2",
    "@remix-run/react": "^2.10.2",
    "@remix-run/serve": "^2.10.2",
    "cookie": "^0.6.0",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-flat-routes": "^0.6.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.10.2",
    "@types/cookie": "^0.6.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.19",
    "clsx": "^2.1.1",
    "eslint": "^9.6.0",
    "eslint-plugin-import-x": "^0.5.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "globals": "^15.8.0",
    "husky": "^9.0.11",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwind-merge": "^2.4.0",
    "tailwindcss": "^3.4.4",
    "typescript": "~5.1.6",
    "typescript-eslint": "8.0.0-alpha.10",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@9.5.0"
}
